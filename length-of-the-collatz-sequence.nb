  RowBox[{
   RowBox[{"CollatzLength", "[", "n_Integer", "]"}], "/;", 
   RowBox[{"n", ">", "0"}]}], ":=", 
  RowBox[{"Length", "@", 
   RowBox[{"NestWhileList", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"EvenQ", "[", "#", "]"}], ",", 
        RowBox[{"Quotient", "[", 
         RowBox[{"#", ",", "2"}], "]"}], ",", 
        RowBox[{
         RowBox[{"3", " ", "#"}], "+", "1"}]}], "]"}], "&"}], ",", "n", ",", 
     RowBox[{
      RowBox[{"#1", "\[NotEqual]", "1"}], "&"}], ",", "1", ",", "Infinity", 
     ",", 
     RowBox[{"-", "1"}]}], "]"}]}]}]], "Input",
